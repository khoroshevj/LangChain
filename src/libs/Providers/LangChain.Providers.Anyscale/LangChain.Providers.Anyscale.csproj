    </PropertyGroup>

    <ItemGroup Label="Usings">
        <Using Include="Anyscale" />
        <Using Include="System.Net.Http" />
    </ItemGroup>

    <PropertyGroup Label="NuGet">
        <Description>Anyscale Endpoints Chat model provider.</Description>
        <PackageTags>$(PackageTags);anyscale;api</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Anyscale" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\LangChain.Providers.Abstractions\LangChain.Providers.Abstractions.csproj" />
    </ItemGroup>

</Project>
